<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="207.0" prefWidth="541.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.arcreane.wordscambler.SelectDifficulty">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <HBox alignment="CENTER" prefHeight="74.0" prefWidth="502.0" spacing="100.0">
        <children>
            <Button fx:id="buttonEasy" onAction="#easy" prefHeight="50.0" prefWidth="100.0" text="Easy"/>
            <Button fx:id="buttonMedium" onAction="#medium" prefHeight="50.0" prefWidth="100.0" text="Medium"/>
            <Button fx:id="buttonHard" onAction="#hard" prefHeight="50.0" prefWidth="100.0" text="Hard"/>
        </children>
    </HBox>
</VBox>
